{
    "name": "saldanhakun/development-bundle",
    "type": "symfony-bundle",
    "description": "Pacote Symfony 7 para facilitar o desenvolvimento, teste e depuração de aplicações",
    "keywords": [],
    "homepage": "https://github.com/saldanhakun/development-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Marcelo Saldanha",
            "email": "saldanha@uttara.com.br"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^8.2",
        "phpdocumentor/reflection-docblock": "^5.6.1",
        "phpstan/phpdoc-parser": "^2.1",
        "phpunit/phpunit": "^9.6.22",
        "symfony/phpunit-bridge": "^7.2"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Saldanhakun\\DevelopmentBundle\\": "src/"
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.2.*"
        }
    }
}
